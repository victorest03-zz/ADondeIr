@using System.Web.Optimization
@{
    ViewBag.Title = "Tipo de Actividad";
    ViewBag.DescriptionPage = "Mantenimiento";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@section styles
{
    @Styles.Render("~/Styles/DataTables")
}
<div class="row">
    <div class="col-sm-12">
        <div class="box box-primary">
            <div class="box-header">
                <h2 class="box-title">Listado</h2>
                <button type="button" class="btn btn-primary pull-right" id="btnNuevaTipoActividad">
                    <span class="fa fa-plus"></span> Nuevo Tipo de Actividad
                </button>
            </div>
            <div class="box-body">
                <table id="tableTipoActividad" class="table table-striped table-bordered table-hover table-responsive"></table>
            </div>
        </div>
    </div>
</div>
@section scripts{
    @Scripts.Render("~/Scripts/DataTables")
    <script src="~/Content/js/TipoActividad/Index.min.js?v=@DateTime.Now.Ticks.ToString()"></script>
}